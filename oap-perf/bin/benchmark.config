#!/bin/bash

# config required variables for benchmark tests
oapVersionNum=3
dataScale=10
dataPartitions=50
testTrie="true"
sparkPath=/home/oap/spark/spark-2.1.0-bin-hadoop2.7
hdfsPath="hdfs:///user/oap/oaptest/"

# "oap" format or "parquet" format or "both"
benchmarkFormats="both"

# 3=11 to test index and naive case, 2=10 to test index only, 1=01 to test naive only
useIndexes=3

# bits from left to right represents if we test B-Tree, Bitmap and Trie index
indexFlag=6

enableStatistic="true"
testOapStrategy="true"
benchmarkTestTimes=3

# flag for profiling between bitmap and btree index, only used for internal test
cmpBitmapAndBtree="true"


# default config of file path
benchmarkJarPath=../target/scala-2.11/TestOap-0.${oapVersionNum}-assembly-1.0.jar
benchmarkResPath=./benchmark_res_tpcds${dataScale}_oap-0.${oapVersionNum}
fileInfoPath=./tableInfo
indexInfoPath=./indexInfo
